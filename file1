int analogPin = A0;
float voltage, temperature;

void setup() {
    Serial.begin(9600);
}

void loop() {
    int sensorValue = analogRead(analogPin);
    voltage = sensorValue * (5.0 / 1023.0);
    temperature = voltage * 100.0;
    Serial.print("Temperature: ");
    Serial.print(temperature);
    Serial.println(" Â°C");
    delay(1000);
}

